# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = tab
indent_size = 4

[*.js]
insert_final_newline = true
indent_style = space
indent_size = 2

[*.py]
insert_final_newline = false
indent_style = space
indent_size = 4

